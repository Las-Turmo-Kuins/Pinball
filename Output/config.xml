<config>
  <app>
    <title>Game Development template</title>
    <maxFrameDuration value="16"/>
  </app>

  <renderer>
    <vsync value="false"/>
  </renderer>

  <window>
    <resolution width="1000" height="1000" scale="1"/>
    <fullscreen value="false "/>
    <borderless value="false"/>
    <resizable value="false"/>
    <fullscreen_window value="false"/>
  </window>

  <audio>
    <music volume="128"/>
    <fx volume="128"/>
  </audio>

  <scene>
    <map path="Assets/Maps/" name="Map_final.tmx"/>
    <player x="720" y="1400" texturepath="Assets/Textures/Player/player_1.png"/>
  </scene>
  <animations>
		<player loop="true" speed="0.075f">
			<animation x="0" y="0" w="32" h="32" />
		</player>
  </animations>
</config>
